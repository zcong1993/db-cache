{
  "name": "@zcong/db-cache",
  "version": "0.4.1",
  "description": "my cool project",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.json && npm run api-types",
    "test": "npm run lint && npm run jest",
    "test:cov": "npm run lint && npm run jest:cov",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "jest": "jest --detectOpenHandles --forceExit",
    "jest:cov": "jest --coverage --detectOpenHandles --forceExit",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "release": "node ./scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "api-types": "api-extractor run"
  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.5",
    "@types/debug": "^4.1.7",
    "@types/ioredis": "^4.26.7",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.6.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "execa": "^5.1.1",
    "husky": "^7.0.1",
    "ioredis": "^4.27.7",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "minimist": "^1.2.5",
    "mysql2": "^2.3.0",
    "prettier": "^2.3.2",
    "prompts": "^2.4.1",
    "semver": "^7.3.5",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0",
    "ts-jest": "^27.0.4",
    "typeorm": "^0.2.36",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "lint-staged": {
    "*.{js,json,ts,md}": "prettier --write"
  },
  "dependencies": {
    "@zcong/node-redis-cache": "^0.7.2",
    "@zcong/singleflight": "^0.3.0",
    "debug": "^4.3.2",
    "lodash": "^4.17.21"
  },
  "bugs": "https://github.com/zcong1993/db-cache/issues",
  "homepage": "https://github.com/zcong1993/db-cache",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcong1993/db-cache.git"
  }
}
