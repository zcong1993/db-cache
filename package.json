{
  "name": "@zcong/db-cache",
  "version": "0.7.0",
  "description": "like go-zero cache, but for NodeJS",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.json && npm run api-types",
    "test": "npm run lint && npm run jest",
    "test:cov": "npm run lint && npm run jest:cov",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write .",
    "jest": "jest --detectOpenHandles --forceExit",
    "jest:cov": "jest --coverage --detectOpenHandles --forceExit",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "release": "js-release",
    "api-types": "api-extractor run"
  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.23.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@zcong/js-release": "^0.2.5",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "ioredis": "^5.0.4",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "mysql2": "^2.3.3",
    "prettier": "^2.6.2",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3",
    "ts-jest": "^28.0.0",
    "typeorm": "^0.3.6",
    "typescript": "^4.6.4"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@zcong/node-redis-cache": "^0.9.0",
    "@zcong/singleflight": "^0.3.0",
    "debug": "^4.3.4",
    "lodash": "^4.17.21"
  },
  "bugs": "https://github.com/zcong1993/db-cache/issues",
  "homepage": "https://github.com/zcong1993/db-cache",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcong1993/db-cache.git"
  },
  "peerDependencies": {
    "typeorm": "^0.3.3"
  }
}
