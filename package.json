{
  "name": "@zcong/db-cache",
  "version": "0.4.2",
  "description": "my cool project",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.json && npm run api-types",
    "test": "npm run lint && npm run jest",
    "test:cov": "npm run lint && npm run jest:cov",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write .",
    "jest": "jest --detectOpenHandles --forceExit",
    "jest:cov": "jest --coverage --detectOpenHandles --forceExit",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "release": "js-release",
    "api-types": "api-extractor run"
  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.17",
    "@types/debug": "^4.1.7",
    "@types/ioredis": "^4.27.8",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@zcong/js-release": "^0.2.5",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^7.0.4",
    "ioredis": "^4.28.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.0",
    "mysql2": "^2.3.2",
    "prettier": "^2.4.1",
    "sequelize": "^6.8.0",
    "sequelize-typescript": "^2.1.1",
    "ts-jest": "^27.0.7",
    "typeorm": "^0.2.38",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@zcong/node-redis-cache": "^0.7.3",
    "@zcong/singleflight": "^0.3.0",
    "debug": "^4.3.2",
    "lodash": "^4.17.21"
  },
  "bugs": "https://github.com/zcong1993/db-cache/issues",
  "homepage": "https://github.com/zcong1993/db-cache",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcong1993/db-cache.git"
  }
